    "@vue/compiler-sfc": "^3.5.13",
    "lodash": "^4.17.21",
    "mermaid": "^11.4.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.1",
    "vue-template-compiler": "^2.7.16"
